    <%= render partial: "index/template/itmedia", locals: { rss_itmedias: @rss_itmedias } %>
    <%= render partial: "index/template/itmatmarkit", locals: { rss_itmatmarkits: @rss_itmatmarkits } %>
    <%= render partial: "index/template/gigazine", locals: { rss_gigazines: @rss_gigazines } %>
    <%= render partial: "index/template/techcrunch", locals: { rss_techcrunchs: @rss_techcrunchs } %>
    <%= render partial: "index/template/engadget", locals: { rss_engadgets: @rss_engadgets } %>
    <%= render partial: "index/template/cnet", locals: { rss_cnets: @rss_cnets } %>
    <%= render partial: "index/template/itmatome", locals: { rss_itmatomes: @rss_itmatomes } %>
<% end %>
</div>
