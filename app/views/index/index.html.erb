    <%= render partial: "index/template/getnews", locals: { rss_getnews: @rss_getnews } %>
    <%= render partial: "index/template/appgiga", locals: { rss_appgigas: @rss_appgigas } %>
    <%= render partial: "index/template/codezine", locals: { rss_codezines: @rss_codezines } %>
